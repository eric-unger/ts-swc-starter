{
  "name": "magna-labs-scheduler",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "build": "swc ./src --out-dir build",
    "watch-compile": "swc src -w --out-dir build",
    "watch-dev": "nodemon --watch \"build/**/*\" -e js ./build/index.js",
    "dev": "concurrently \"pnpm watch-compile\" \"pnpm watch-dev\"",
    "start": "NODE_ENV=production node build/main.js",
    "clean": "rm -rf build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.22",
    "@types/node": "^18.11.12",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
